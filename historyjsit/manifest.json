{
	"name": "History.js It!",
	"version": "1.0",
	"description": "History.js It! As a Google Chrome Extension\nProject Page: http://github.com/balupton/Facebook-Cleaner",
	"update_url": "http://github.com/balupton/historyjsit.crx/raw/master/updates.xml",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"browser_action": {
		"default_icon": "icon19disabled.png"
	},
	"content_scripts": [
		{
			"matches": ["http://*/*"],
			"js": ["content.js"]
		}
	],
	"permissions": ["tabs","http://*/*"],
	"background_page": "background.html"
}
